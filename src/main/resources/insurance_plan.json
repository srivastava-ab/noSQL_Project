{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "_type": {
      "type": "string"
    },
    "creationDate": {
      "type": "string"
    },
    "planCostShares": {
      "type": "object",
      "properties": {
        "copay": {
          "type": "string"
        },
        "_type": {
          "type": "string"
        },
        "deductible": {
          "type": "string"
        }
      },
      "required": [
        "copay",
        "_type",
        "deductible"
      ]
    },
    "linkedPlanServices": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_type": {
            "type": "string"
          },
          "planserviceCostShares": {
            "type": "object",
            "properties": {
              "copay": {
                "type": "string"
              },
