                "type": "string"
              },
              "deductible": {
                "type": "string"
              }
            },
            "required": [
              "copay",
              "_type",
              "deductible"
            ]
          },
          "linkedService": {
            "type": "object",
            "properties": {
              "_type": {
                "type": "string"
              }
            },
            "required": [
              "_type"
            ]
          }
        },
        "required": [
          "_type",
          "planserviceCostShares",
          "linkedService"
        ]
      }
